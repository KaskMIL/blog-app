<h1 class='title'>Users</h1>
<ul class='users-list'>
<%@users.each do |user|%>
<a href="/users/<%= user.id %>" id="<%= user.id %>">
    <li class='user-container'>
      <img src=<%= user.photo %> alt="Image placeholder">
      <div class='user-data'>
        <h2><%= user.name %></h2>
        <p>Posts: <%= user.postCounter %></p>
      </div>
    </li>
  </a>
<%end%>
</ul>